<h2>Locations</h2>
<div id="index-list">
	<% @locations.each do |location| %>
	  <%= link_to "#{location.tapes.count}", location, class: "label" %> <%= link_to "#{location.location}", location %> <br />
	<% end %><br />
</div>
<div id="index-btn">
	<%= link_to 'New location', new_location_path, class: "btn btn-primary" %>	
</div>